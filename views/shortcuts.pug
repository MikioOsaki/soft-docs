            td(class="mdl-data-table__cell--non-numeric") Browser
            td(class="mdl-data-table__cell--non-numeric") essentiell
          tr
            td(class="mdl-data-table__cell--non-numeric") Strg + r
            td(class="mdl-data-table__cell--non-numeric") Webseite neu laden (reload)
            td(class="mdl-data-table__cell--non-numeric") Browser
            td(class="mdl-data-table__cell--non-numeric") nice to know
          tr
            td(class="mdl-data-table__cell--non-numeric") Strg + +
            td(class="mdl-data-table__cell--non-numeric") Zoom vergrößern
            td(class="mdl-data-table__cell--non-numeric") Browser
            td(class="mdl-data-table__cell--non-numeric") nice to know
          tr
            td(class="mdl-data-table__cell--non-numeric") Strg + -
            td(class="mdl-data-table__cell--non-numeric") Zoom verringern
            td(class="mdl-data-table__cell--non-numeric") Browser
            td(class="mdl-data-table__cell--non-numeric") nice to know
          tr
            td(class="mdl-data-table__cell--non-numeric") Strg + 0
            td(class="mdl-data-table__cell--non-numeric") Zu Zoomstufe 0 wechseln (100%)
            td(class="mdl-data-table__cell--non-numeric") Browser
            td(class="mdl-data-table__cell--non-numeric") nice to know
          tr
            td(class="mdl-data-table__cell--non-numeric") F9
            td(class="mdl-data-table__cell--non-numeric") Webseite im Lesemodus anzeigen und bei erneutem Drücken wieder zurück
            td(class="mdl-data-table__cell--non-numeric") Browser
            td(class="mdl-data-table__cell--non-numeric") nice to know
          tr
            td(class="mdl-data-table__cell--non-numeric") Strg + h
            td(class="mdl-data-table__cell--non-numeric") Browser-Historie (Chronik) anzeigen (also alle jemals besuchten Webseiten)
            td(class="mdl-data-table__cell--non-numeric") Browser
            td(class="mdl-data-table__cell--non-numeric") nice to know
          tr
            td(class="mdl-data-table__cell--non-numeric") Strg + b
            td(class="mdl-data-table__cell--non-numeric") Browser-Lesezeichen anzeigen
            td(class="mdl-data-table__cell--non-numeric") Browser
            td(class="mdl-data-table__cell--non-numeric") nice to know
          tr
            td(class="mdl-data-table__cell--non-numeric") Alt + →
            td(class="mdl-data-table__cell--non-numeric") Eine Seite vor
            td(class="mdl-data-table__cell--non-numeric") Browser
            td(class="mdl-data-table__cell--non-numeric") nice to know
          tr
            td(class="mdl-data-table__cell--non-numeric") Alt + ←
            td(class="mdl-data-table__cell--non-numeric") Eine Seite zurück
            td(class="mdl-data-table__cell--non-numeric") Browser
            td(class="mdl-data-table__cell--non-numeric") nice to know
